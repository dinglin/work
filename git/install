# ssh-keygen
ssh-keygen
# id_rsa.pub 上传到 git.corp.anjuke.com
echo $HOME"/.ssh/id_rsa.pub"


sudo apt-get install git -y



echo "please input your name on git:"
read name
git config --global user.name $name
echo "please input your email on git:"
read email
git config --global user.email $email

.git-config

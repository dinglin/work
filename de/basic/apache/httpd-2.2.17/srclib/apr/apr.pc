prefix=/usr/local/httpd-2.2.x
exec_prefix=/usr/local/httpd-2.2.x
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/usr/local/httpd-2.2.x/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.4.2
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lrt -lcrypt  -lpthread -ldl
Cflags: -D_REENTRANT -D_GNU_SOURCE -g -O2 -pthread -I${includedir}
